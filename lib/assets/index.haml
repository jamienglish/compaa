!!!
%head
  %script{src: '/underscore.js'}
  %script{src: '/jquery-2.0.0.js'}
  %script{src: '/context_blender.js'}
  %script{src: '/compaa.js'}
  %link{rel: 'stylesheet', href: '/bootstrap.css'}
  :javascript
    var compaa;
    $(function() {
      compaa = new Compaa();
      compaa.init();
    });

%body
  %canvas#offScreenCanvas{style: 'position: absolute; left: -9999999px;'}
  %canvas#difference

  %img#animation{style: 'display: none'}
  %img#oldImage{style: 'display: none'}
  %img#newImage{style: 'display: none'}

  %div{style: 'width: 400px; margin: 0px auto; text-align: center;'}
    %p
      %a.btn#differenceButton{href: '#'} Difference
      %a.btn#animationButton{href: '#'} Animation
      %a.btn#oldImageButton{href: '#'} Old Image
      %a.btn#newImageButton{href: '#'} New Image
    %p
      %a#accept.btn.btn-primary{href: '#'} Accept
      %a#reject.btn.btn-danger{href: '#'}  Reject
